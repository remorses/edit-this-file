        "babel-plugin-edit-this-page": "*",
        "dotenv": "^8.2.0",
        "mocha": "^6.1.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "sucrase": "^3.12.1",
        "typescript": "^3.8.3"
    },
    "peerDependencies": {
        "@chakra-ui/core": "^0.8",
